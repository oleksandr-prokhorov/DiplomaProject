@page "{login}"
@model QueriesToDBTest.Pages.Admin.AdministratorModel
@{
    Layout = "./_Employee_Layout";
}
<h2>Вітаємо, @Model.Login</h2><br />
<div class="row">
    <div class="col-md-4">
        <img src="~/adm_accounts.png" width="128" height="128" >
        <h3>Облікові записи співробітників</h3>
        <p> Перейдіть на цю форму, щоб зареєструвати нового користувача або змінити дані вже зареєстрованого
        </p>
        <p> <a class="btn btn-primary btn-sm" asp-page="AdministratorAccounts" asp-route-login=@Model.Login>Перейти до роботи з обліковими записами користувачів </a></p>
    </div>
    
</div>
